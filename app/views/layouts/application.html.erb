<!DOCTYPE html>
<html>
  <head>
    <title></title>
    <%= csrf_meta_tag %>
    <%= render 'layouts/stylesheets' %>
  </head>
  <body>
    <div class="container">
      <%= render 'layouts/header' %>
      <section class="round">
        <% flash.each do |name, msg| %>
          <%= content_tag :div, msg %>
        <%end %>

        <%= yield %>
      </section>
    </div>
    <%= render 'layouts/footer' %>
  </body>
</html>
